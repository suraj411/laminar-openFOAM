FoamFile
{
    format      binary;
    class       volVectorField;
    location    "0/fluid";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 5 0);

boundaryField
{
    inlet_fluid
    {
        type            fixedValue;
        value           uniform (0 5 1);
    }
    outlet_fluid
    {
        type            pressureInletOutletVelocity;
        value           nonuniform List<vector> 
0
;
    }
    walls_fluid
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    fluid_to_fins
    {
        type            noSlip;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (0 5 0);
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform (0 5 0);
    }
}


