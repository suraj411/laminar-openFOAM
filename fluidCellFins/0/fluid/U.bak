FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0/fluid";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform (0 5 0);

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet_fluid
    {
        type            fixedValue;
        value           uniform (0 5 1);
    }
    outlet_fluid
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }
	walls_fluid
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    fluid_to_fins
    {
        type            noSlip;
    }
}

