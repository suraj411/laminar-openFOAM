FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0/cell3";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 4;

boundaryField
{
    walls_cell
    {
        type            zeroGradient;
    }
    cell3_to_fins
    {
        type            calculated;
        value           uniform 0;
    }
}


